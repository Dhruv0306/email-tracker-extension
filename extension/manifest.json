{
    "manifest_version": 3,
    "name": "Email Tracker",
    "version": "0.1.0",
    "permissions": [
        "notifications",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}